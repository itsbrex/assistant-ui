{
  "name": "@assistant-ui/react-llamaindex",
  "version": "0.0.1",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.mts",
        "default": "./dist/server.mjs"
      },
      "require": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      }
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/server.ts --format cjs,esm --dts --sourcemap"
  },
  "dependencies": {
    "@ai-sdk/react": "^0.0.4",
    "@assistant-ui/react-ai-sdk": "^0.1.7",
    "ai": "^3.2.5",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@assistant-ui/react": "^0.1.9",
    "@types/react": "*",
    "react": "^18",
    "llamaindex": "0.3.16"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@assistant-ui/react": "workspace:*",
    "@assistant-ui/react-ai-sdk": "workspace:*",
    "@assistant-ui/tsconfig": "workspace:*",
    "@types/node": "^20.14.7",
    "tsup": "^8.1.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://assistant-ui.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yonom/assistant-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Yonom/assistant-ui/issues"
  }
}
